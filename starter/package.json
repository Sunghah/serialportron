{
  "name": "serialportron",
  "productName": "Serialportron",
  "author": "Sung Hah Hwang",
  "version": "1.0.0",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunghah/serialportron.git"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "postinstall": "./node_modules/.bin/electron-rebuild --target=2.0.4",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron:windows": "electron-packager . serialportron --overwrite --asar=true --platform=win32 --arch=all --prune=true --out=release-builds",
    "electron:linux": "electron-packager . serialportron --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e"
  },
  "private": false,
  "dependencies": {
    "@agm/core": "1.0.0-beta.2",
    "@angular/cdk": "6.1.0",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/platform-server": "6.0.2",
    "@angular/router": "6.0.2",
    "@types/serialport": "^6.0.3",
    "bootstrap": "^4.1.2",
    "core-js": "2.4.1",
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0",
    "serialport": "^6.2.1",
    "stream-http": "github:node-influx/stream-http",
    "uglify-js": "^3.3.26",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.2",
    "@types/bootstrap": "3.3.32",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.73",
    "codelyzer": "4.3.0",
    "electron": "2.0.4",
    "electron-builder": "20.14.7",
    "electron-rebuild": "^1.8.1",
    "electron-reload": "^1.2.2",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "npm-run-all": "^4.1.3",
    "protractor": "^5.3.2",
    "ts-node": "2.0.0",
    "tslint": "5.9.1",
    "typescript": "~2.7.0",
    "update": "^0.7.4",
    "wait-on": "^2.1.0"
  }
}